{
  "name": "akitainu",
  "version": "0.1.0-alpha.6",
  "description": "Gradual code linting tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/cli/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "watch:build": "tsc --build --watch",
    "watch:test": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "install": "node -e 'require(\"fs\").existsSync(\"./dist\") || process.exit(1)' || npm run build"
  },
  "keywords": [
    "akitainu"
  ],
  "files": [
    "dist",
    "!dist/.tsbuildinfo",
    "src"
  ],
  "author": "uhyo <uhyo@uhy.ooo>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@types/yargs": "^15.0.13",
    "babel-jest": "^26.6.3",
    "jest": "^27.0.0-next.9",
    "typescript": "^4.3.0-beta"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "yargs": "^16.2.0"
  }
}